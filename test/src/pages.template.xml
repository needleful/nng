<templates>
	<template name='page'>
		<param name='numbers' type='list'>
			<param name='number' type='integer'/>
		</param>
		<content>
			<ul>
				<foreach list='numbers'>
					<li>[[number+1]] [[number*number-1]] [[lt(number, 1); gt(number, 5)]] [[gt(number, 0), lt(number, 5)]]</li>
				</foreach>
			</ul>
		</content>
	</template>
</templates>