<templates>
	<template name='page'>
		<param name='title' type='text'/>
		<param name='custom-nav-list' type='list' required='false'>
			<param name='link' type='text'/>
		</param>
		<content>
			<html>
				<h1>[[title]]</h1>
				<ul>
					<foreach list='custom-nav-list'>
						<li>/<a href="[[link]]">[[link]]</a></li>
					</foreach>
				</ul>
			</html>
		</content>
	</template>
</templates>